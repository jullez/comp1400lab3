    printf("*\n **\n ***\n   My name is Julio\n   ***\n **\n*");

    int num1;
    int num2;
    int sum;
    int subtract,divide,multiply;
    printf("Julio Cinci,\n110015225\n");
    printf("Enter first number: ");
    scanf("%d", &num1);
    printf("Enter second number: ");
    scanf("%d", &num2);
    sum = num1 + num2;
    subtract=num1-num2;
    divide=num1/num2;
    multiply=num1*num2;
    printf("-----------------------------------------------------------------\n|\t\t\t\t\t\t\t\t |\n|\t\t\t%d+%d=%d\t\t\t\t\t |\n|\t\t\t\t\t\t\t\t | \n------------------------------------------------------------------\n",num1,num2,sum);
   printf("%d-%d=%d\n%d*%d=%d\n%d/%d=%d",num1,num2,subtract,num1,num2,multiply,num1,num2,divide);

    return 0;
}
